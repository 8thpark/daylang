{
  "name": "daylang",
  "private": true,
  "type": "module",
  "watch": {
    "build": {
      "patterns": [
        "ui"
      ],
      "extensions": "css,html,jsx,png,txt"
    }
  },
  "scripts": {
    "build": "vite build ./ui --outDir ../dist --emptyOutDir",
    "check-deps": "depcheck",
    "dev": "vite ./ui",
    "format": "prettier --write './**/*.{js,jsx,css,html,json}'",
    "format:check": "prettier --check './**/*.{js,jsx,css,html,json}'",
    "lint": "eslint './**/*.{js,jsx}'",
    "lint:fix": "eslint './**/*.{js,jsx}' --fix",
    "watch": "npm-watch"
  },
  "dependencies": {
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.1.0",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.12.10",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.36.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "npm-watch": "^0.13.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^5.4.0"
  }
}
